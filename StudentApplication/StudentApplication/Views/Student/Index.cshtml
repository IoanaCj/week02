@model List<StudentApplication.Models.Student>

<div>
    This is a list of Students <br />

   <table style="width:100%">
        <thead>
            <tr>
                <th>Id</th>
                <th>StudentName</th>
                <th>Age</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var student in Model)
            {
                <tr>
                    <td>@student.Id</td>
                    <td>@student.StudentName</td>
                    <td>@student.Age</td>
                    <td><a href="/student/details/@student.Id">Click here</a></td>
                    <td><a href="/student/edit/@student.Id">Edit</a></td>
                </tr>
            }
        </tbody>
    </table>
</div>